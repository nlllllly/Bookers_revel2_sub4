 
		<div class="col-xs-9">
			<h2>Follow Users</h2>
			<!--users一覧 -->
			<% if current_user.following_user.exists? %>
				<table class="table">
					<thead>
						<tr>
							<th>image</th>
							<th>name</th>
							<th colspan="3"></th>
							<th colspan="3"></th>
						</tr>
					</thead>

					<tbody>
						<% @users.each do |user| %>
						<tr>
							<td><%= attachment_image_tag(user, :profile_image, :fill, 50, 50, fallback: "no-image-mini.jpg") %></td>
							<td><%= user.name%></td>
							<td><%= link_to "Show", user_path(user), class: "user_#{user.id}" %></td>
							<td><%= link_to 'フォロー外す', unfollow_path(user.id), method: :POST %></td>
						</tr>
						<% end %>
					</tbody>
				</table>
				<!--users一覧 -->
			<% else %>
				<h6>ユーザはいません</h6>
			<% end %>
		</div>